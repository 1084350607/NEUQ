.title {
		font-weight: 900;
		font-size: 25px;
		margin-top: 40px;
		text-align: center;
}
.regsister_wrapper {
		margin-top: 40px;
}
.auth_wrapper {
		position: relative;
		width: 85vw;
		margin: 5px auto;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: start;
		-webkit-justify-content: flex-start;
		        justify-content: flex-start;
		height: 45px;
		border-bottom: 1px solid #ccc;
}
.auth_btn {
		position: absolute;
		right: 0;
		top: 50%;
		-webkit-transform: translateY(-50%);
		        transform: translateY(-50%);
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		font-size: 10px;
		width: 90px;
		height: 30px;
		border: 0px;
		background-color: #FFFFFF;
		/* border-left: 1px solid #ccc; */
}
.item_title {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		padding: 0 10px;
		width: 70px;
		font-size: 15px;
}
input {
		padding: 0;
		height: 100%;
		line-height: 100%;
}
.comfirm_btn {
		width: 85vw;
		margin-top: 40rpx;
		background-color: #55b9f3;
}
.server {
		margin-top: 40rpx;
		color: #CCCCCC;
		font-size: 20rpx;
		text-align: center;
}

