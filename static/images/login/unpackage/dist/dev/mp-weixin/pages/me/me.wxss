.header{
	padding: 0 20rpx 120rpx 50rpx;
	
	padding: 25rpx 20rpx 20rpx 50rpx;
	
	width: 100%;
	height: 200rpx;
	background-color: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	box-sizing: border-box;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.avatar{
	height: 150rpx;
	width: 150rpx;
	border-radius: 50%;
	/*防止头像挤压*/
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	/*10%灰白度*/
	-webkit-filter: grayscale(10%);
	        filter: grayscale(10%);
}
.info_wrapper{
	width: 80%;
	height: 80%;
	margin-left: 40rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.regs_login{
	margin-top: 30rpx;
	font-size: 17px;
}
.nickname{
	font-size: 19px;
	color: #777777;
}
.nav_info{
	font-size: 15px;
	color: #777777;
}
.setting{
	height: 30rpx;
	width: 30rpx;
}

